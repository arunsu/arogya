{
  "name": "AAIMS",
  "version": "0.0.0",
  "description": "My test website",
  "main": "index.js",
  "scripts": {
    "build:dev": "npm run clean:public && webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "npm run clean:public && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run build:dev",
    "rimraf": "rimraf",
    "clean:public": "npm run rimraf -- public",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm start",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "server:prod": "http-server public -p 3000 -c-1 --cors",
    "server": "npm run server:dev",
    "start": "npm run server:dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arunsu/arogya.git"
  },
  "author": "arunsu",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^3.3.7",
    "css-loader": "^0.28.7",
    "d3": "^4.12.0",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "jquery": "^3.2.1",
    "morgan": "^1.5.3",
    "pug": "^2.0.0-rc.4",
    "pug-html-loader": "^1.1.5",
    "react": "^16.2.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
}
